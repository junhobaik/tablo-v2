{
  "manifest_version": 2,
  "name": "Tablo v2 (beta)",
  "description": "Website scraps and management and Feed(RSS) subscriptions",
  "version": "1.0.8",
  "icons": {
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "Tablo v2",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage"]
}
